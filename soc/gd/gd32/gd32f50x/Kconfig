config SOC_SERIES_GD32F50X
	bool
	select ARM
	select CPU_HAS_ARM_MPU
	select CPU_HAS_FPU
	select CPU_CORTEX_M33
	select ARMV8_M_DSP
	select GD32_HAS_AF_PINMUX
	select GD32_HAS_IRC_40K
	select SOC_EARLY_INIT_HOOK
	help
	  Enable support for GigaDevice GD32F50x MCU series
